    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.1",
    "@mui/x-date-pickers": "^6.18.5",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
